with [get-view "Project Explorer" | get-tree] {
    select "task/mainline-vim.c" | key-type "M2+ARROW_UP"
    select "task/fork-vim.c" "task/mainline-vim.c"
    get-menu "Compare With/Each Other" | click
}
with [get-editor "Compare ('task/fork-vim.c' - 'task/mainline-vim.c')"] {
    get-text-viewer | set-caret-pos 1 37
    with [get-text-viewer -after [get-label "task/mainline-vim.c"] -index 1] {
        set-caret-pos 3 1
        select-range 2 1 3 1
        key-type "M1+c"
    }
    with [get-text-viewer] {
        set-caret-pos 6 1
        select-range 5 1 6 1
        key-type "M1+v"
        set-caret-pos 14 1
        key-type Enter
        type-text "#ifdef FEAT_CMDL_COPL"
        key-type "M2+BS" -times 2
        type-text MPL
        set-caret-pos 23 1
        type-text "#endif"
        key-type Enter
        set-caret-pos 7 1
        select-range 6 19 7 1
        key-type "M1+a"
        key-type "M1+c"
        key-type Right
    }
}
