+  0x040,		                /* set display start line to 0 */
+  0x090,		                /* no fixed lines */
+  0x089,		                /* RAM access control  */
+  
+  0x0af,		                /* disable sleep mode  */
+  0x0a4,		                /* normal display  */
+  0x0a5,		                /* display all points, ST7565, UC1610 */
+  U8G_ESC_DLY(100),       /* delay 100 ms */
