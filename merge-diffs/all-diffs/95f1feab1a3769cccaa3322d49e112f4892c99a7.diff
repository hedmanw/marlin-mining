+static uint8_t u8g_usart_spi_out(uint8_t data)
+{
+  /* send data */
+  UDR1 = data;
+  /* wait for empty transmit buffer */
+  while(!(UCSR1A & (1 << UDRE1)));
+
+  return  UDR1;
+}
+
