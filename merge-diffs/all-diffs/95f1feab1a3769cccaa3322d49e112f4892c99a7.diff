+void u8g_ssd1351_hicolor_to_stream(uint8_t *ptr)
+{
+  register uint8_t cnt = RGB332_STREAM_BYTES;
+  register uint8_t low, high, r, g, b;
+  uint8_t *dest = u8g_ssd1351_stream_bytes;
+  for( cnt = 0; cnt < RGB332_STREAM_BYTES; cnt++ )
+  {
+    low = *ptr++;
+    high = *ptr++;
+    
+    r = high & ~7;
+    r >>= 2;
+    b = low & 31;
+    b <<= 1;
+    g = high & 7;
+    g <<= 3;
+    g |= (low>>5)&7;
+    
+    *dest++ = r;
+    *dest++ = g;
+    *dest++ = b;
+  } 
+}
