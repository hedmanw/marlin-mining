+#include "Marlin.h"
+#include "temperature.h"
+
+void safe_delay(uint16_t ms) {
+  while (ms > 50) {
+    ms -= 50;
+    delay(50);
+    thermalManager.manage_heater();
+  }
+  delay(ms);
+}
